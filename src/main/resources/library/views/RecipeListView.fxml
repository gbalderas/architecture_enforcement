<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.views.RecipeListView">
   <children>
      <ListView fx:id="recipesList" maxHeight="1.7976931348623157E308" maxWidth="300.0" onMouseClicked="#clickedItem" VBox.vgrow="ALWAYS" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="addRecipeButton" mnemonicParsing="false" onAction="#addNewRecipe" text="Add Recipe" />
            <Button fx:id="editRecipeButton" mnemonicParsing="false" text="Edit Recipe" />
        </buttons>
      </ButtonBar>
   </children>
</VBox>
